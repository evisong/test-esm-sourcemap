{
  "buildCommand": "yarn build",
  "framework": null,
  "functions": {
    "api/index.js": {
      "includeFiles": "static/hidden-sourcemap/**/*"
    }
  },
  "headers": [
    {
      "source": "/static/hidden-sourcemap/scripts/preact.esm.js",
      "headers": [
        {
          "key": "X-My-Test",
          "value": "Value"
        }
      ]
    }
  ],
  "rewrites": [{ "source": "/api/(.*)", "destination": "/api" }]
}
